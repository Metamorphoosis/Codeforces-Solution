#include <bits/stdc++.h>
using namespace std;

int main () {
    int n, h, p, width = 0;
    cin >> n >> h;

    for (int i = 0; i < n; i++) {
        cin >> p;
        if(p > h) {
            width += 2;
        }
        else {
            width++;
        }   
    }
    cout << width << endl;
    return 0;
}
